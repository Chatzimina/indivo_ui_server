<% _(all_apps).each(function(app, i) {
	var input_id = "app_" + app.id.replace(/-/g, '_'); %>

	{% if jsonData.6.z_score < 0%}
	      	<% if (app.name =='Medications') {%>
			 <div class="app <%= app.enabled ? '' : '' %> <%= app.autonomous ? 'autonomous' : '' %>" style="background-image: url('<%=  app.getImageSource() %>');" <%= app %>>
		<% } else { %>
			<div class="app <%= app.enabled ? '' : 'disabled' %> <%= app.autonomous ? 'autonomous' : '' %>" style="background-image: url('<%=  app.getImageSource() %>');" <%= app %>>
		<% } %>
		
		<b class="app_name"><%= app.name %></b>
	
		<div class="app_info">
			<b><%= app.name %></b>
			<div class="app_description"><%= app.description %></div>
			<span><%= app.app_id %></span>
		
		</div>
		<% if (app.name =='Medications') {%> 	
		<div class="app_options">
			<input type="hidden"  id="<%= input_id %>" name="enable_app" value="0" <%= app.enabled = 'checked="checked"'%> />
                        <input type="checkbox" id="<%= input_id %>" name="enable_app" value="1" <%= app.enabled ? 'checked="checked"' : '' %> <%= app %> /> 
                        <label for="<%= input_id %>">Enable</label>
		
                </div>
		 <% } else { %>
		 <div class="app_options">
                        <input type="checkbox" id="<%= input_id %>" name="enable_app" value="1" <%= app.enabled ? 'checked="checked"' : '' %> <%= app %> /> 
                        <label for="<%= input_id %>">Enable</label>

                </div>
		 <% } %>
		
	{%endif%}
	</div>
<% }) %>
<div style="clear: left;"> </div>
